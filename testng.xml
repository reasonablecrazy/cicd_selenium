<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="2">
	
  <test thread-count="5" name="E2E Tests">
    <classes>
      <class name="naveennarayananacademy.tests.StandAloneTest">
      	<methods>
      		<include name ="submitTest"/>
      		<include name ="validateProductOnConfirmationPage"/>
      	</methods>
      </class>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="Error Messages">
    <classes>
      <class name="naveennarayananacademy.tests.StandAloneTest">
      	<methods>
      		<include name ="incorrectLogin"/>
      	</methods>
      </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
